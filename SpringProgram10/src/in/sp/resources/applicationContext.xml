<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

      <!-- bean definitions here -->
    <bean class="in.sp.beans.Address" id="adrId2">
        <property name="houseno" value="564"/>
        <property name="city" value="Washim"/>
        <property name="pincode" value="444586"/>
     </bean>
     <bean class="in.sp.beans.Address" id="adrId1"  autowire-candidate="false">
        <property name="houseno" value="564"/>
        <property name="city" value="Washim"/>
        <property name="pincode" value="444586"/>
     </bean>
       <bean class="in.sp.beans.Student" id="stdId" autowire="byType"><!--used setter method internaly-->
        <property name="rollno" value="101"/>
        <property name="name" value="Harish"/>
        <!--<constructor-arg ref="addrId"/>-->
     </bean>
</beans>